<% values.each_pair do |klass, groups| -%>
<%=klass %>
<% groups.each_pair do |group, scopes| -%>
<% scopes.each_pair do |scope, contexts| -%>
<% contexts.each_pair do |context, methods| -%>
<% if methods.size > 0 -%>
<%= "#{context} #{scope} methods [#{methods.size}]: #{methods.join(', ')}"%>
<% end -%>
<% end -%>
<% end -%>
<% end -%>
<% end -%>
